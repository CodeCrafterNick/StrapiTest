{
  "kind": "collectionType",
  "collectionName": "quotes",
  "info": {
    "singularName": "quote",
    "pluralName": "quotes",
    "displayName": "Quotes",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "text"
    },
    "politician": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::politician.politician",
      "inversedBy": "quotes"
    },
    "date": {
      "type": "date"
    },
    "citation": {
      "type": "string",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "context": {
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "citationVerified": {
      "type": "boolean"
    },
    "shortID": {
      "type": "customField",
      "options": {
        "uuid-format": "^[a-zA-Z0-9_]{5,15}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    }
  }
}
