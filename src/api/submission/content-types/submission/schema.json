{
  "kind": "collectionType",
  "collectionName": "submissions",
  "info": {
    "singularName": "submission",
    "pluralName": "submissions",
    "displayName": "Submissions",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "text"
    },
    "politician": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::politician.politician"
    },
    "date": {
      "type": "date"
    },
